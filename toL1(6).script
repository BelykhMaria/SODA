   GMAT SC.SE_E.VMAG = 10.74;
   GMAT SC.SE_E.FPA = 90;
   GMAT SC.SE_E.RA = RA(I);
   GMAT SC.SE_E.DEC = DEC(I);
   GMAT SC.SE_E.AZI = AZI(I);
   GMAT to_L1.Element1 = 0;
   
   GMAT SC0 = SC;
   
