      Maneuver to_L1(SC);
      
      Propagate DefaultProp(SC) {SC.ElapsedDays = 30};
      Propagate 'Propagate2' DefaultProp(SC) {SC.SE_L1.X = -1400000, SC.SE_E.X = -10000, StopTolerance = 1};
      
